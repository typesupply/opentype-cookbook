# ---------------------
# Fractions: Individual
# ---------------------

feature ss06 {
    @slash = [slash fraction];

    lookup FractionNumerators {
        sub @figures by @figuresNumerator;
    } FractionNumerators;

    sub [@slash @figuresDenominator] @figuresNumerator' by @figuresDenominator;
    sub slash by fraction;
} ss06;


